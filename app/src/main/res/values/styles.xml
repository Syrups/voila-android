<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <style name="Tenveux.Widget.ActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">

        <item name="android:background" tools:ignore="NewApi">@color/red</item>
        <item name="background">@color/red</item>

        <item name="android:colorControlNormal" tools:ignore="NewApi">
            @color/wallet_holo_blue_light
        </item>

    </style>

    <style name="Tenveux.Widget.ActionBar.NOActionBar" >

        <item name="android:windowActionBar">false</item>
        <item name="windowActionBar">false</item>

    </style>

    <!-- ACTION BAR STYLES -->
    <style name="Tenveux.Widget.ActionBar.overlay">

        <item name="android:windowActionBarOverlay">true</item>
        <item name="windowActionBarOverlay">true</item>

        <item name="colorPrimary">@android:color/transparent</item>

    </style>


    <style name="Tenveux.Widget.ActionBar.Transparent">
        <item name="android:background">@android:color/transparent</item>
    </style>


    <style name="ButtonBigStyle">
        <item name="fontPath">fonts/SansitaOne.ttf</item>
        <item name="android:background">@drawable/btn_bg_take</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>
    </style>


</resources>

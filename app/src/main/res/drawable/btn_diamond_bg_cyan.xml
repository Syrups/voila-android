<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">

        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="45">


            <shape android:shape="rectangle">

                <solid android:color="@color/cyan" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="9dp"
        android:left="9dp"
        android:right="9dp"
        android:top="9dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="90">
            <shape
                android:shape="line"
                android:top="5dip">
                <stroke
                    android:width="1dip"
                    android:color="#FFFFFF" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="9dp"
        android:left="9dp"
        android:right="9dp"
        android:top="9dp">
        <shape
            android:shape="line"
            android:top="5dip">
            <stroke
                android:width="1dip"
                android:color="#FFFFFF" />
        </shape>
    </item>


</layer-list>